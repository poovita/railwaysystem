@model IEnumerable<Rail.Models.priority>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Priority1)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.train_type)
        </th>
        <th></th>
    </tr>



    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Priority1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.train_type)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Priority1 }) |
                @Html.ActionLink("Details", "Details", new { id = item.Priority1 }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Priority1 })
            </td>
        </tr>
    }

</table>
